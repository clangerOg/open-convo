{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.ts",
  "scripts": {
    "dev": "cross-env NODE_PATH=. ts-node-dev --transpile-only --require dotenv/config ./src/index.ts",
    "start": "cross-env NODE_PATH=dist node dist/index.js",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm *.tsbuildinfo",
    "test": "jest --setupFiles dotenv/config",
    "lint": "eslint . --quiet",
    "format:write": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@prisma/client": "5.15.1",
    "@swc/core": "^1.6.1",
    "@swc/helpers": "^0.5.11",
    "@types/express": "4.17.17",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "express": "4.18.2",
    "jsonwebtoken": "^9.0.2",
    "ts-node-dev": "2.0.0",
    "tsup": "6.6.3",
    "typescript": "4.9.4",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@open-convo/eslint-config": "workspace:*",
    "@open-convo/typescript-config": "workspace:*",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "dotenv": "16.0.3",
    "prisma": "^5.15.1",
    "regenerator-runtime": "0.13.11"
  }
